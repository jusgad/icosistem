{
  "name": "ecosistema-emprendimiento",
  "version": "1.0.0",
  "description": "Plataforma integral para gestiÃ³n de emprendedores y ecosistema de emprendimiento",
  "main": "app/static/src/js/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:prod": "NODE_ENV=production webpack --mode production --optimize-minimize",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "dev": "webpack serve --mode development --open",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint app/static/src/js/**/*.js --fix",
    "lint:css": "stylelint app/static/src/scss/**/*.scss --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "webpack-bundle-analyzer dist/bundle.js",
    "clean": "rimraf app/static/dist/*",
    "format": "prettier --write app/static/src/**/*.{js,css,scss}",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "postinstall": "npm run security:audit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/company/ecosistema-emprendimiento.git"
  },
  "keywords": [
    "emprendimiento",
    "entrepreneurship",
    "flask",
    "python",
    "webapp",
    "business-platform"
  ],
  "author": "Ecosistema Emprendimiento Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not dead",
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "aos": "^2.3.4",
    "axios": "^1.7.2",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.3",
    "cleave.js": "^1.6.0",
    "datatables.net": "^2.0.8",
    "datatables.net-bs5": "^2.0.8",
    "dropzone": "^6.0.0-beta.2",
    "flatpickr": "^4.6.13",
    "intro.js": "^7.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prismjs": "^1.29.0",
    "select2": "^4.1.0-rc.0",
    "socket.io-client": "^4.7.5",
    "sweetalert2": "^11.12.0",
    "swiper": "^11.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.39",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "sass": "^1.77.5",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/frontend/setup.js"
    ],
    "collectCoverageFrom": [
      "app/static/src/js/**/*.js",
      "!app/static/src/js/**/*.test.js",
      "!app/static/src/js/vendor/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "testMatch": [
      "**/tests/frontend/**/*.test.js",
      "**/app/static/src/**/*.test.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "browser": true,
      "es2021": true,
      "jquery": true,
      "jest": true
    },
    "globals": {
      "Chart": "readonly",
      "io": "readonly",
      "Swal": "readonly",
      "axios": "readonly",
      "moment": "readonly",
      "_": "readonly",
      "bootstrap": "readonly"
    },
    "rules": {
      "no-console": "warn",
      "no-debugger": "error",
      "no-unused-vars": "warn",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard-scss",
      "stylelint-config-prettier-scss"
    ],
    "rules": {
      "scss/at-import-partial-extension": null,
      "scss/at-import-no-partial-leading-underscore": null
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "printWidth": 100
  },
  "security": {
    "audit": {
      "level": "moderate",
      "exclude": []
    }
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/ecosistema-emprendimiento"
  }
}
